{
  "name": "@curi-bio/frontend-test-utils",
  "version": "0.0.49",
  "description": "Various utilities to help with Front End unit and Visual Regression testing.",
  "main": "./dist/frontend-test-utils.common.js",
  "author": "Curi Bio <contact@curibio.com> (http://www.curibio.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriBio/frontend-test-utils.git"
  },
  "scripts": {
    "test:e2e": "node ./check_engines.js && testcafe chrome:headless:cdpPort=9222 ./tests/e2e --screenshots path=./tests/e2e/screenshots",
    "prepare": "pip install -r requirements-dev.txt && pre-commit install && npx npm-merge-driver install",
    "build": "node ./check_engines.js && npm run _build",
    "test:unit-no-prebuild": "node ./check_engines.js && jest -c jest.config.js",
    "test:unit": "npm run build && jest -c jest.config.js",
    "_build": "vue-cli-service build --target lib --name frontend-test-utils ./src/index.js"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@platform-os/testcafe-helpers": "^2.3.2",
    "bootstrap": "^4.4.1",
    "chrome-remote-interface": "^0.28.0",
    "ci-info": "^2.0.0",
    "cross-env": "^7.0.2",
    "d-path-parser": "^1.0.0",
    "fs-extra": "^9.0.1",
    "mkdirp": "^1.0.4",
    "resemblejs": "^3.2.5",
    "testcafe": "^1.9.3",
    "testcafe-vue-selectors": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime-corejs2": "7.9.2",
    "@vue/cli-service": "^4.4.4",
    "@vue/test-utils": "^1.0.3",
    "autoprefixer": "^9.7.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "~26.5.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "core-js": "2.6.11",
    "css-loader": "^3.4.2",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-snakecasejs": "^2.2.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "~7.0.0",
    "jest": "~26.4.2",
    "jest-html-reporter": "~3.1.3",
    "jest-serializer-vue": "~2.0.2",
    "jest-transform-stub": "~2.0.0",
    "jquery": "~3.5.1",
    "jsdoc": "~3.6.3",
    "npm-merge-driver": "~2.3.6",
    "popper.js": "~1.16.0",
    "sinon": "~9.1.0",
    "vue": "~2.6.11",
    "vue-template-compiler": "~2.6.10",
    "webpack": "~4.44.2"
  },
  "license": "MIT"
}
